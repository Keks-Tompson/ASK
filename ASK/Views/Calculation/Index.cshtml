                                            {
                                                <option value = "1">CO₂</option>
                                            }
                                        }
                                            
                                        @if(GlobalStaticSettingsASK.SensorRange.SO2.Is_Used)
                                        {
                                            @if(GlobalStaticSettingsASK.CalculationSetting.TypeViews == TypeCalulationViews.SO2) 
