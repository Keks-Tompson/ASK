                        <td></td>
                    </tr>
                    
                    <tr>
                        <td>
                            <label style="font-size: 140%;">Методика расчётов</label>
                        </td>

                        <td>
                            <form method="post">
                                
                                <label style="font-size: 140%;">Расчёты онлайн&nbsp</label>

                                <label>
                                    <select class="form-select" aria-label="" name="number" 
                                        style=" color: #808080;
                                                font-size: 90%;

                                                font-family: Times New Roman , Times, serif;
                                    ">

                                        @if(GlobalStaticSettingsASK.SensorRange.CO.Is_Used)
                                        {
